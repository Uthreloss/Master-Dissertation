<launch>
    <!-- Pepper Responsive Node-->
    <node name="pepper_controller" pkg="tf_broadcaster" type="pepper.py" respawn="false" output="screen" >
    </node>
    <!-- Astra Camera Node-->
    <node pkg="astra_body_tracker" type="astra_body_tracker_node" name="astra_body_tracker_node" output="screen">
    </node>
    <!-- Pepper Code Node-->
    <node name="pepper_Orbbec" pkg="tf_broadcaster" type="my_pepper.py" respawn="false" output="screen" >
    </node>
    <!-- Pepper Responsive Node-->
    <node name="body_data_saver" pkg="tf_broadcaster" type="data_storing_flag.py" respawn="false" output="screen" >
    </node>

    <!-- RVIZ node -->
    <!--node pkg="rviz" type="rviz" name="$(anon rviz)" args="~/.rviz/pepper.rviz" /-->
    <!-- https://github.com/cse481sp17/cse481c/wiki/Lab-11:-Visualizations-in-ROS-with-RViz -->
  </launch>
